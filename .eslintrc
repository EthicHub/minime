{
    "extends": "airbnb",
    "rules": {
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/anchor-is-valid": [
            "warn",
            {
                "aspects": [
                    "invalidHref"
                ]
            }
        ]
    },
    "overrides": [
        {
            "files": [
                "contracts.js"
            ],
            "rules": {
                "import/no-unresolved": "off",
                "import/no-extraneous-dependencies": "off",
                "global-require": "off",
                "import/no-dynamic-require": "off"
            }
        }
    ]
}